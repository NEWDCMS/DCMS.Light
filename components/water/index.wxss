.water-box {
  display: flex;
  flex-direction: column;
  align-items: center;
  background: transparent;
  box-sizing: border-box;
  padding-bottom: 160rpx;
}
.water-container{
  position: fixed;
  right: 100rpx;
  bottom: 300rpx;
  z-index: 2;
  width: 250rpx;
  height: 250rpx;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.water-img{
  width: 230rpx;
  height:150rpx;
}

