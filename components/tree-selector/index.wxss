.float-container {
    position: fixed;
    left: 0rpx;
    bottom: 0rpx;
    z-index: 2;
    width: 100%;
    height: 100rpx;
    box-shadow: 9px 4px 0px 0px rgba(155, 193, 185, 0.5);
    background-color: #eee;
}

.float-container-toolbar {
    display: flex;
    width: 100%;
    height: 100rpx;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
}

.float-container-toolbar .left-block {
    width: 50%;
    height: 100rpx;
    color: #3B5998;
    font-size: 30rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}


.float-container-toolbar .right-block {
    width: 50%;
    height: 100rpx;
    color: #fff;
    font-size: 30rpx;
    text-align: center;
    background: #3B5998;
    display: flex;
    align-items: center;
    justify-content: center;
}


.tree-warpper
{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    height: 100%;
    padding: 0 10rpx;
    box-sizing: border-box;
    background-color: #fff;
}

.tree-selector {
    display: flex;
    flex-direction: column;
    align-items: center;
    background-color: #fff;
}

.search-tree {
    width: 650rpx;
    margin: 20rpx auto;
    border: #d3d3d3 solid 1px;
    border-radius: 10rpx;
    padding: 10rpx 20rpx;
    display: flex;
}

.search-tree .search-input {
    width: 95%;
    font-size: 30rpx;
}

.search-tree .search_btn {
    line-height: 20rpx;
}

