.container {
    /* display: flex;
    flex-direction: column; */
    align-items: center;
    background-color: #fff;
}
.content {
    width: 750rpx;
    height: 320rpx;
    padding: 20rpx 20rpx;
    box-sizing: border-box;
    border-bottom: 1rpx solid #ddd;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.container-title,
.l-title-class {
    color: #bbb;
}
.img {
    margin-left: 20rpx;
    margin-right: 30rpx;
}
.line {
    height: 1rpx;
    width: 100%;
    background-color: #ddd;
    margin: 5rpx 0 5rpx 0;
}
.des {
    width: 100%;
    height: 260rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}
.des-title {
    font-size: 30rpx;
    color: #333;
    flex-wrap: wrap;
    font-weight: bold;
}
.des-mobile {
    color: #bbb;
    font-size: 30rpx;
    font-weight: bold;
}
.des-address {
    font-size: 30rpx;
}
.des-info {
    color: #bbb;
}
.des-distance-time {
    font-size: 30rpx;
    color: #bbb;
}
.btn-cls {
    display: inline-flex;
    align-items: center;
    justify-content: center;
    white-space: nowrap;
    font-size: 28rpx;
    position: relative;
    color: #fff;
    padding: 12rpx;
    border: none;
    background-color: #3B5998
}
.des-distance {
    display: flex;
    align-items: center;
    color: #bbb;
}
.des-distance-text {
    font-size: 30rpx;
}
.btn-mobile {
    border: 1rpx #3B5998 solid;
    background-color: white;
    /* border-radius: 40rpx 0rpx 0rpx 40rpx; */
    min-width: 250rpx;
    margin-right: 10rpx;
    height: 50rpx;
    color: #3B5998;
}
.des-distance-icon {
    margin-right: 9rpx;
}
.btn-signin {
    min-width: 250rpx;
    /* border-radius: 0rpx 40rpx 40rpx 0rpx; */
    margin-left: 10rpx;
    height: 50rpx
}
.btn-select {
    background: #53a245;
}
.terminal-filter-type {
    padding: 30rpx;
    border-bottom: 1rpx #ddd solid;
}
.terminal-search {
    padding: 30rpx 0rpx 30rpx 0rpx;
}
.terminal-filter-type-title {
    display: flex;
    font-weight: bold;
    justify-content: space-between;
    font-size: 30rpx;
    color: rgba(69, 82, 107, 1);
    padding: 15rpx 0;
}
.terminal-filter-tags {
    display: flex;
    flex-wrap: wrap;
}
.terminal-filter-tag {
    margin-right: 15rpx;
    margin-bottom: 15rpx;
}
/* ========================= */
.float-container {
    position: fixed;
    right: 40rpx;
    bottom: 60rpx;
    z-index: 2;
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    box-shadow: 0px 3px 8px 0px rgba(155, 193, 185, 0.5);
    display: flex;
    justify-content: center;
    background-color: #fff;
    align-items: center;
}
.float-img {
    /* width: 50rpx;
    height: 50rpx; */
    color: #3B5998;
}
.float-txt {
    font-size: 28rpx;
    color: #3B5998;
    margin-top: 15rpx;
}
.float-container-left { 
    position: fixed; 
    right: 150rpx; 
    bottom: 60rpx; 
    z-index: 2; 
    width: 100rpx; 
    height: 100rpx; 
    border-radius: 50%; 
    box-shadow: 0px 3px 8px 0px rgba(155, 193, 185, 0.5); 
    display: flex; 
    justify-content: center; 
    background-color: #fff; 
    align-items: center; 
    text-align: left; 
} 

.maps{
    width:100%;
    height:100vh;
}